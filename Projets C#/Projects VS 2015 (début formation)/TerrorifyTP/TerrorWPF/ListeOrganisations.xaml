<UserControl x:Class="TerrorWPF.ListeOrganisations"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TerrorWPF"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Margin="10" Background="WhiteSmoke">
        <TextBlock Style="{StaticResource PhraseListe}"  Text="Liste des organisations"></TextBlock>
        <ComboBox Margin="10 0"></ComboBox>
    </StackPanel>
</UserControl>
